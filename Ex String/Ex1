const string1 = 'w3resource';
let result1 = true;
if (typeof string1 == 'string') {
    result1 = true
  }
  else result1 = false;
console.log(result1);

const string2 = [1, 2, 4, 0];
let result2 = true;
if (typeof string2 == 'string') {
    result2 = true
  }
  else result2 = false;
console.log(result2);
